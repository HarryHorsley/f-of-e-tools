#! /usr/bin/vvp
:ivl_version "10.2 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1abb3f0 .scope module, "half_adder_procedural_tb" "half_adder_procedural_tb" 2 5;
 .timescale -9 -11;
P_0x1aba6e0 .param/l "period" 1 2 11, +C4<00000000000000000000000000010100>;
v0x1ae0880_0 .var "a", 0 0;
v0x1ae0940_0 .var "b", 0 0;
v0x1ae0a10_0 .net "carry", 0 0, L_0x1ae0d90;  1 drivers
v0x1ae0b10_0 .var "clk", 0 0;
v0x1ae0bb0_0 .net "sum", 0 0, L_0x1aa77a0;  1 drivers
E_0x1ac9920 .event posedge, v0x1ae0b10_0;
S_0x1abb5e0 .scope module, "UUT" "half_adder" 2 13, 3 3 0, S_0x1abb3f0;
 .timescale -9 -11;
    .port_info 0 /INPUT 1 "a"
    .port_info 1 /INPUT 1 "b"
    .port_info 2 /OUTPUT 1 "sum"
    .port_info 3 /OUTPUT 1 "carry"
L_0x1aa77a0 .functor XOR 1, v0x1ae0880_0, v0x1ae0940_0, C4<0>, C4<0>;
L_0x1ae0d90 .functor AND 1, v0x1ae0880_0, v0x1ae0940_0, C4<1>, C4<1>;
v0x1abb7b0_0 .net "a", 0 0, v0x1ae0880_0;  1 drivers
v0x1ae05b0_0 .net "b", 0 0, v0x1ae0940_0;  1 drivers
v0x1ae0670_0 .net "carry", 0 0, L_0x1ae0d90;  alias, 1 drivers
v0x1ae0710_0 .net "sum", 0 0, L_0x1aa77a0;  alias, 1 drivers
    .scope S_0x1abb3f0;
T_0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1ae0b10_0, 0, 1;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1ae0b10_0, 0, 1;
    %delay 2000, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1abb3f0;
T_1 ;
    %wait E_0x1ac9920;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1ae0880_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1ae0940_0, 0, 1;
    %delay 2000, 0;
    %load/vec4 v0x1ae0bb0_0;
    %pad/u 32;
    %cmpi/ne 0, 0, 32;
    %flag_mov 8, 4;
    %load/vec4 v0x1ae0a10_0;
    %pad/u 32;
    %cmpi/ne 0, 0, 32;
    %flag_or 4, 8;
    %jmp/0xz  T_1.0, 4;
    %vpi_call 2 36 "$display", "test failed for input combination 00" {0 0 0};
T_1.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1ae0880_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1ae0940_0, 0, 1;
    %delay 2000, 0;
    %load/vec4 v0x1ae0bb0_0;
    %pad/u 32;
    %cmpi/ne 1, 0, 32;
    %flag_mov 8, 4;
    %load/vec4 v0x1ae0a10_0;
    %pad/u 32;
    %cmpi/ne 0, 0, 32;
    %flag_or 4, 8;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 42 "$display", "test failed for input combination 01" {0 0 0};
T_1.2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1ae0880_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1ae0940_0, 0, 1;
    %delay 2000, 0;
    %load/vec4 v0x1ae0bb0_0;
    %pad/u 32;
    %cmpi/ne 1, 0, 32;
    %flag_mov 8, 4;
    %load/vec4 v0x1ae0a10_0;
    %pad/u 32;
    %cmpi/ne 0, 0, 32;
    %flag_or 4, 8;
    %jmp/0xz  T_1.4, 4;
    %vpi_call 2 48 "$display", "test failed for input combination 10" {0 0 0};
T_1.4 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1ae0880_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1ae0940_0, 0, 1;
    %delay 2000, 0;
    %load/vec4 v0x1ae0bb0_0;
    %pad/u 32;
    %cmpi/ne 0, 0, 32;
    %flag_mov 8, 4;
    %load/vec4 v0x1ae0a10_0;
    %pad/u 32;
    %cmpi/ne 1, 0, 32;
    %flag_or 4, 8;
    %jmp/0xz  T_1.6, 4;
    %vpi_call 2 54 "$display", "test failed for input combination 11" {0 0 0};
T_1.6 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1ae0880_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1ae0940_0, 0, 1;
    %delay 2000, 0;
    %load/vec4 v0x1ae0bb0_0;
    %pad/u 32;
    %cmpi/ne 1, 0, 32;
    %flag_mov 8, 4;
    %load/vec4 v0x1ae0a10_0;
    %pad/u 32;
    %cmpi/ne 1, 0, 32;
    %flag_or 4, 8;
    %jmp/0xz  T_1.8, 4;
    %vpi_call 2 60 "$display", "test failed for input combination 01" {0 0 0};
T_1.8 ;
    %vpi_call 2 62 "$stop" {0 0 0};
    %jmp T_1;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
